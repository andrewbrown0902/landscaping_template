permalink: /contact/
---


<!DOCTYPE html>
<html class="light" lang="en"><head>
<meta charset="utf-8"/>
<meta content="width=device-width, initial-scale=1.0" name="viewport"/>
<title>{{ site.company.name }} | Get Your Free Quote</title>
<script src="https://cdn.tailwindcss.com?plugins=forms,container-queries"></script>
<link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:wght,FILL@100..700,0..1&amp;display=swap" rel="stylesheet"/>
<link href="https://fonts.googleapis.com/css2?family=Manrope:wght@200;300;400;500;600;700;800&amp;display=swap" rel="stylesheet"/>
<script id="tailwind-config">
        tailwind.config = {
            darkMode: "class",
            theme: {
                extend: {
                    colors: {
                        "primary": "#11d452",
                        "background-light": "#f6f8f6",
                        "background-dark": "#102216",
                    },
                    fontFamily: {
                        "display": ["Manrope", "sans-serif"]
                    },
                    borderRadius: {
                        "DEFAULT": "0.25rem",
                        "lg": "0.5rem",
                        "xl": "0.75rem",
                        "full": "9999px"
                    },
                },
            },
        }
    </script>
<style type="text/tailwindcss">
        .material-symbols-outlined {
            font-variation-settings: 'FILL' 0, 'wght' 400, 'GRAD' 0, 'opsz' 24;
        }
        body {
            font-family: 'Manrope', sans-serif;
        }
        .form-input-focus {
            @apply focus:ring-primary focus:border-primary border-gray-300 dark:border-white/10 dark:bg-white/5;
        }
    </style>
</head>
<body class="bg-background-light dark:bg-background-dark text-[#0d1b12] dark:text-white transition-colors duration-300">
{% include "header.njk" %}

<main>
<section class="bg-[#eef5f0] dark:bg-[#0c1a11] py-16 md:py-24">
<div class="max-w-[1280px] mx-auto px-6 text-center">
<h1 class="text-4xl md:text-6xl font-black mb-6 tracking-tight leading-[1.1]">
                Get Your Free Quote
            </h1>
<p class="text-lg md:text-xl font-medium text-gray-600 dark:text-gray-400 max-w-2xl mx-auto">
                Transform your outdoor space with {{ site.company.city }}'s premier landscaping team. No-obligation estimates and expert response within 24 hours.
            </p>
</div>
</section>
<section class="max-w-[1280px] mx-auto px-6 py-16">
<div class="grid grid-cols-1 lg:grid-cols-2 gap-16">
<div>
<div class="bg-white dark:bg-[#1a3321] p-8 md:p-10 rounded-2xl shadow-xl border border-[#e7f3eb] dark:border-[#24462d]">
<h3 class="text-2xl font-bold mb-8">Project Inquiry</h3>
<form action="#" class="space-y-6">
<div class="grid grid-cols-1 md:grid-cols-2 gap-6">
<div>
<label class="block text-sm font-bold mb-2 uppercase tracking-wider text-gray-500">Full Name</label>
<input class="w-full rounded-lg border-gray-300 dark:border-[#24462d] dark:bg-[#102216] dark:text-white focus:border-primary focus:ring-primary h-12" placeholder="John Doe" type="text"/>
</div>
<div>
<label class="block text-sm font-bold mb-2 uppercase tracking-wider text-gray-500">Phone Number</label>
<input class="w-full rounded-lg border-gray-300 dark:border-[#24462d] dark:bg-[#102216] dark:text-white focus:border-primary focus:ring-primary h-12" placeholder="(317) 000-0000" type="tel"/>
</div>
</div>
<div>
<label class="block text-sm font-bold mb-2 uppercase tracking-wider text-gray-500">Email Address</label>
<input class="w-full rounded-lg border-gray-300 dark:border-[#24462d] dark:bg-[#102216] dark:text-white focus:border-primary focus:ring-primary h-12" placeholder="john@example.com" type="email"/>
</div>
<div>
<label class="block text-sm font-bold mb-2 uppercase tracking-wider text-gray-500">Service Selection</label>
<select class="w-full rounded-lg border-gray-300 dark:border-[#24462d] dark:bg-[#102216] dark:text-white focus:border-primary focus:ring-primary h-12">
<option value="">Select a service...</option>
<option value="hardscaping">Hardscaping &amp; Patios</option>
<option value="softscaping">Softscaping &amp; Planting</option>
<option value="lawn-care">Lawn Care &amp; Maintenance</option>
<option value="lighting">Outdoor Lighting</option>
<option value="pool-design">Pool Design</option>
</select>
</div>
<div>
<label class="block text-sm font-bold mb-2 uppercase tracking-wider text-gray-500">Project Details (Optional)</label>
<textarea class="w-full rounded-lg border-gray-300 dark:border-[#24462d] dark:bg-[#102216] dark:text-white focus:border-primary focus:ring-primary" placeholder="Tell us about your vision..." rows="4"></textarea>
</div>
<button class="w-full bg-primary hover:bg-primary/90 text-[#0d1b12] py-4 rounded-xl font-black text-lg transition-all shadow-lg hover:shadow-primary/20 transform hover:-translate-y-1 uppercase tracking-tight" type="submit">
                            Send My Quote Request
                        </button>
</form>
</div>
</div>
<div class="flex flex-col justify-between">
<div>
<h3 class="text-3xl font-black mb-10">Direct Contact</h3>
<div class="space-y-10">
<div class="flex items-start gap-5">
<div class="bg-primary/10 p-3 rounded-lg text-primary">
<span class="material-symbols-outlined">call</span>
</div>
<div>
<p class="text-sm font-bold uppercase tracking-widest text-gray-500 mb-1">Call Anytime</p>
<a class="text-2xl font-extrabold hover:text-primary transition-colors" href="tel:{{ site.company.phone_raw }}">{{ site.company.phone_display }}
</a>
</div>
</div>
<div class="flex items-start gap-5">
<div class="bg-primary/10 p-3 rounded-lg text-primary">
<span class="material-symbols-outlined">mail</span>
</div>
<div>
<p class="text-sm font-bold uppercase tracking-widest text-gray-500 mb-1">Email Us</p>
<a class="text-2xl font-extrabold hover:text-primary transition-colors" href="mailto:{{ site.company.email }}">{{ site.company.email }}</a>
</div>
</div>
<div class="flex items-start gap-5">
<div class="bg-primary/10 p-3 rounded-lg text-primary">
<span class="material-symbols-outlined">schedule</span>
</div>
<div>
<p class="text-sm font-bold uppercase tracking-widest text-gray-500 mb-1">Business Hours</p>
<ul class="text-lg font-medium space-y-1">
<li class="flex justify-between w-64"><span>Mon â€” Fri</span> <span class="font-bold">8am - 6pm</span></li>
<li class="flex justify-between w-64"><span>Saturday</span> <span class="font-bold">9am - 2pm</span></li>
<li class="flex justify-between w-64 text-gray-400"><span>Sunday</span> <span class="font-bold">Closed</span></li>
</ul>
</div>
</div>
</div>
</div>
<div class="mt-16 pt-10 border-t border-[#e7f3eb] dark:border-[#1a3321] flex flex-wrap gap-8 items-center">
<div class="flex items-center gap-3 bg-white dark:bg-[#1a3321] px-6 py-4 rounded-xl border border-[#e7f3eb] dark:border-[#1a3321] shadow-sm">
<div class="flex text-yellow-400">
<span class="material-symbols-outlined fill-1">star</span>
<span class="material-symbols-outlined fill-1">star</span>
<span class="material-symbols-outlined fill-1">star</span>
<span class="material-symbols-outlined fill-1">star</span>
<span class="material-symbols-outlined fill-1">star</span>
</div>
<div>
<p class="text-sm font-black">Google Rated 5.0</p>
<p class="text-[10px] text-gray-500 uppercase tracking-tighter">Based on 250+ reviews</p>
</div>
</div>
<div class="flex items-center gap-3 bg-white dark:bg-[#1a3321] px-6 py-4 rounded-xl border border-[#e7f3eb] dark:border-[#1a3321] shadow-sm">
<div class="size-10 rounded-full bg-primary/20 flex items-center justify-center text-primary">
<span class="material-symbols-outlined">verified_user</span>
</div>
<div>
<p class="text-sm font-black">Licensed &amp; Insured</p>
<p class="text-[10px] text-gray-500 uppercase tracking-tighter">Certified Professional Team</p>
</div>
</div>
</div>
</div>
</div>
</section>
<section class="w-full px-6 md:px-20 pb-20">
<div class="max-w-[1280px] mx-auto rounded-3xl overflow-hidden border border-[#e7f3eb] dark:border-[#1a3321] shadow-2xl relative h-[450px]">
<div class="absolute inset-0 bg-[#d9e8df] dark:bg-[#14281a] flex items-center justify-center overflow-hidden">
<div class="absolute inset-0 bg-cover bg-center opacity-40 grayscale" style='background-image: url("https://lh3.googleusercontent.com/aida-public/AB6AXuBVlu3X3f-xoUOrcp5yp2aVQtu4V9HfHuAruZrIIVwEnSBd2eVf_GJHR2gcEpzE5TnbJRqdiBZ6R1Lb1Ie_W0WwG5feHMD-27W1qtOAomCU_M6fVETHpCmpc5rW6n2epGAIb1e6cBRt7IhMBtb-MDdVHKa2Zv8-Z5fRc8ah64Wsror0UH04JH5g6ysA96XB8iqhR_2uksNJE9Gv8F9tW4mqzqNh9eWRxg8vPbCitMlEm1WbqUcyd63ptcYcAhHselVv4X8WkWU8ikag");'></div>
<div class="relative z-10 text-center px-4">
<div class="inline-flex items-center gap-2 bg-white dark:bg-[#1a3321] px-6 py-3 rounded-full shadow-lg mb-4">
<span class="material-symbols-outlined text-red-500">location_on</span>
<span class="font-bold">Serving All of {{ site.company.city }} &amp; Surrounding Areas</span>
</div>
<div class="flex flex-wrap justify-center gap-3 mt-4">
  {% for area in site.company.areas_served %}
    <span class="px-4 py-1 bg-white/80 dark:bg-[#1a3321]/80 rounded-full text-xs font-bold">
      {{ area | replace(", IN", "") | replace(", OH", "") }}
    </span>
  {% endfor %}
</div>
</div>
</div>
<div class="absolute inset-0 bg-black/5 flex items-center justify-center pointer-events-none">
<button class="bg-primary text-[#0d1b12] px-6 py-2 rounded-lg font-bold shadow-lg pointer-events-auto">View Service Map</button>
</div>
</div>
</section>
</main>

{% include "header.njk" %}


</body></html>
